{
  "name": "learning-git-2",
  "version": "1.0.0",
  "description": "https://wojciechgumulski.github.io/learning-git-2/",
  "main": "index.js",
  "dependencies": {
    "autoprefixer-cli": "^1.0.0",
    "nu-html-checker": "^0.1.0",
    "package.json": "^2.0.1"
  },
  "devDependencies": {
    "browser-sync": "^2.23.5"
  },
  "scripts": {
    "watch:browser-sync": "node_modules/.bin/browser-sync start --server --files ""css/*.css" "*.html""",
    "build:clean":  "rimraf <.css>",
    "build:sass": "node-sass --output-style compact -o css sass",
    "watch:sassprefixer": "npm-run-all onchange -d 100 css/style.css -- node-sass --output-style expanded --source-map true -o css sass --watch -- autoprefixer-cli -o css/style.prefixed.css css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WojciechGumulski/learning-git-2.git"
  },
